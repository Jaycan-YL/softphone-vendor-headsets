{
  "name": "softphone-vendor-headsets",
  "version": "0.1.0",
  "author": "Genesys",
  "module": "dist/softphone-vendor-headsets.esm.js",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:src && npm run build:es && npm run build:module",
    "build:src": "tsc -p . && node copy-version.js",
    "build:es": "tsc -p . --outDir ./dist/es --target es2015 --module es2015",
    "build:module": "webpack && webpack --env.production",
    "pretest": "npm run install-chromium && npm run start-mock-server",
    "posttest": "npm run stop-mock-server",
    "start-mock-server": "npm run stop-mock-server && ./scripts/start-mock-server.sh",
    "stop-mock-server": "./scripts/stop-mock-server.sh",
    "test:complete": "npm run lint && npm run test:unit --collectCoverage=true --runInBand",
    "test:coverage": "jest --collectCoverage=true --runInBand",
    "test:unit": "NODE_ENV=test jest --runInBand",
    "test:watch": "jest --watch --collectCoverage=false --runInBand",
    "lint": "eslint -c .eslintrc.json --ext .ts src",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "browserama": "^3.2.0",
    "fetch-jsonp": "^1.2.1",
    "jest-fetch-mock": "^3.0.3",
    "nock": "^13.1.3",
    "rxjs": "^6.5.5",
    "strict-event-emitter-types": "^2.0.0",
    "utils": "^0.3.1",
    "uuid": "^3.4.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.168",
    "@types/nock": "11.1.0",
    "@types/node": "^12.12.37",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-loader": "^8.2.2",
    "eslint": "^7.32.0",
    "husky": "^4.0.10",
    "ts-jest": "^27.0.5",
    "ts-node": "~7.0.0",
    "tslib": "^2.3.1",
    "typescript": "~4.0.5",
    "webpack-cli": "^3.3.12"
  },
  "husky": {
    "hooks": {
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
